{
  "name": "@eclipse-glsp/theia-integration",
  "version": "2.6.0-next",
  "description": "Glue code to integrate GLSP clients into Eclipse Theia",
  "keywords": [
    "theia-extension",
    "eclipse",
    "graphics",
    "diagram",
    "modeling",
    "visualization",
    "glsp",
    "diagram editor"
  ],
  "homepage": "https://www.eclipse.org/glsp/",
  "bugs": "https://github.com/eclipse-glsp/glsp/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-glsp/glsp-theia-integration.git"
  },
  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "author": {
    "name": "Eclipse GLSP"
  },
  "contributors": [
    {
      "name": "Eclipse GLSP Project",
      "email": "glsp-dev@eclipse.org",
      "url": "https://projects.eclipse.org/projects/ecd.glsp"
    }
  ],
  "main": "lib/browser/index",
  "files": [
    "lib",
    "src",
    "css"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib *.tsbuildinfo ",
    "generate:index": "glsp generateIndex src/browser src/common src/node -s -f",
    "lint": "eslint --ext .ts,.tsx ./src",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@eclipse-glsp/client": "next",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@types/ws": "^8.5.4"
  },
  "peerDependencies": {
    "@theia/core": "^1.65.0",
    "@theia/filesystem": "^1.65.0",
    "@theia/messages": "^1.65.0",
    "@theia/monaco": "^1.65.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/theia-integration-frontend-module",
      "backend": "lib/node/theia-integration-backend-module"
    }
  ]
}
