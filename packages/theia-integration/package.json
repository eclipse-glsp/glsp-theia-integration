{
  "name": "@eclipse-glsp/theia-integration",
  "version": "0.9.0",
  "description": "Glue code to integrate GLSP clients into Eclipse Theia",
  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "engines": {
    "yarn": "1.0.x || >=1.2.1",
    "node": ">=10.11.0 <13"
  },
  "keywords": [
    "theia-extension",
    "eclipse",
    "graphics",
    "diagram",
    "modeling",
    "visualization",
    "glsp",
    "diagram editor"
  ],
  "homepage": "https://www.eclipse.org/glsp",
  "bugs": "https://github.com/eclipse-glsp/glsp-theia-integration/issues",
  "author": {
    "name": "EclipseSource"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-glsp/glsp-theia-integration.git"
  },
  "files": [
    "lib",
    "src",
    "css"
  ],
  "dependencies": {
    "@eclipse-glsp/client": "^0.9.0-next.a088c7ff",
    "@eclipse-glsp/protocol": "^0.9.0-next.a088c7ff",
    "@theia/messages": "^1.0.0",
    "sprotty-theia": "^0.10.0-next.f26a7d7"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc && yarn run lint",
    "lint": "eslint -c ./.eslintrc.js --ext .ts ./src",
    "upgrade:next": "yarn upgrade @eclipse-glsp/client@next @eclipse-glsp/protocol@next sprotty-theia@next",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/glsp-frontend-module",
      "backend": "lib/node/glsp-backend-module"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index",
  "types": "lib/index"
}
